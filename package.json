{
    "name": "gottschalg-theme",
    "displayName": "Gottschalg Theme",
    "description": "Gottschalg color theme",
    "version": "0.1.0",
    "publisher": "gottschalg",
    "license": "MIT",
    "engines": {
        "vscode": "^1.38.0"
    },
    "maintainers": [
        "Igor Gottschalg <igor@gottschalg.mobi>"
    ],
    "categories": [
        "Themes"
    ],
    "homepage": "https://github.com/igorgottschalg/Gottschalg-vscode-theme/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/igorgottschalg/Gottschalg-vscode-theme"
    },
    "bugs": {
        "url": "https://github.com/igorgottschalg/Gottschalg-vscode-theme/issues"
    },
    "keywords": [
        "dark",
        "theme",
        "color-theme",
        "front-end",
        "gotschalg"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#3c4557",
        "theme": "dark"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "themes": [
            {
                "label": "Gottschalg Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/Gottschalg Theme-color-theme.json"
            }
        ]
    },
    "extensionDependencies": [
        "be5invis.vscode-custom-css"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "tslint": "^5.12.1",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    }
}
